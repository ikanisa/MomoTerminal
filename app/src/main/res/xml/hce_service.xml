<?xml version="1.0" encoding="utf-8"?>
<host-apdu-service xmlns:android="http://schemas.android.com/apk/res/android"
    android:description="@string/hce_service_description"
    android:requireDeviceUnlock="false">
    <!-- 
    AID (Application Identifier) for Mobile Money Terminal
    Using a custom AID in the proprietary range (A0 00 00 00 04)
    F0 4D 4F 4D 4F = Custom identifier for MOMO (Mobile Money)
    -->
    <aid-group
        android:category="payment"
        android:description="@string/aid_group_description">
        <aid-filter android:name="A0000000044F4D4F" />
    </aid-group>
</host-apdu-service>
